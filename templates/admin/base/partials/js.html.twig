<script src="{{ asset('dist/compiled/app.js') }}"></script>
{% if text_editor is defined and text_editor == true %}
    <script src="{{ asset('vendor/ckeditor/ckeditor.js') }}"></script>
{% endif %}
{% if dt_script is defined %}
    {% if dt_script == 'v2' %}
        <script src="{{ asset('dist/compiled/app_dt_advance.js') }}"></script>
    {% elseif dt_script == 'v1' %}
        <script src="{{ asset('dist/compiled/app_dt_simple.js') }}"></script>
    {% endif %}
{% endif %}
{% if date_picker is defined and date_picker == true %}
    <script src="{{ asset('vendor/flatpickr/flatpickr.min.js') }}"></script>
    <script src="{{ asset('assets/js/yearpicker.js') }}" async></script>
{% endif %}
